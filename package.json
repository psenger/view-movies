{
  "name": "view-movies",
  "version": "1.0.3",
  "description": "ReactJS Movie Viewing Website",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psenger/view-movies.git"
  },
  "keywords": [
    "ReactJS",
    "Movies",
    "Viewer"
  ],
  "author": "Philip A Senger <philip.a.senger@cngrgroup.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psenger/view-movies/issues"
  },
  "homepage": "https://github.com/psenger/view-movies#readme",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.0",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "0.28.1",
    "dotenv": "4.0.0",
    "es6-promise": "^4.1.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "font-awesome": "^4.7.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.3",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "react": "^15.5.4",
    "react-dev-utils": "^3.0.0",
    "react-dom": "^15.5.4",
    "react-error-overlay": "^1.0.7",
    "react-lazy-load": "^3.0.12",
    "react-player": "^0.18.0",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.3",
    "sass-loader": "^6.0.6",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "^5.94.0",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "prestart": "cp -r ./node_modules/font-awesome/fonts ./public/static/ && cp -r ./node_modules/font-awesome/css ./public/static/",
    "start": "node scripts/start.js",
    "prebuild": "cp -r ./node_modules/font-awesome/fonts ./public/static/ && cp -r ./node_modules/font-awesome/css ./public/static/",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "clean": "rm -rf ./build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "body-parser": "^1.20.3"
  }
}
